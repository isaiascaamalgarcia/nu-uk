    private void setTableCajero(){
        Object[][] cont={
            {null,null,null},
            {null,null,null},
            {null,null,null},
            {null,null,null},
            {null,null,null},
            {null,null,null}
        };
        String[] head={"Nombre","Contraseña","Sucursal"};
        DefaultTableModel mod=new DefaultTableModel(cont,head);
        this.ListaCajeros.setModel(mod);
    }
    private void setTableContador(){
        Object[][] cont={
            {null,null},
            {null,null},
